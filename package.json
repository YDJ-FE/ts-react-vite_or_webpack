{
    "name": "ts-react-webpack4",
    "version": "1.0.0",
    "description": "an admin starter-template with typescript, react, mobx and webpack4...",
    "main": "index.js",
    "scripts": {
        "test": "jest --coverage",
        "add": "add-component",
        "lint": "tslint --type-check --project ./tsconfig.json",
        "dev": "cross-env APP_ENV=dev webpack-dev-server --config build/webpack.config.js --mode development --open --hot",
        "build": "webpack --config build/webpack.config.js --mode production",
        "qa": "cross-env APP_ENV=qa npm run build",
        "prod": "cross-env APP_ENV=prod npm run build"
    },
    "keywords": [
        "webpack4",
        "typescript",
        "admin",
        "react",
        "mobx",
        "starter-template"
    ],
    "author": "jackple",
    "license": "MIT",
    "dependencies": {
        "antd": "^3.8.1",
        "axios": "^0.18.0",
        "bourbon": "^5.1.0",
        "echarts": "^4.1.0",
        "echarts-for-react": "^2.0.14",
        "lodash": "^4.17.10",
        "mobx": "^5.0.3",
        "mobx-react": "^5.2.5",
        "mobx-react-router": "^4.0.4",
        "path-to-regexp": "^2.3.0",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-hot-loader": "^4.3.5",
        "react-loadable": "^5.4.0",
        "react-router-dom": "^4.2.2"
    },
    "devDependencies": {
        "@svgr/webpack": "^2.2.0",
        "@types/classnames": "^2.2.6",
        "@types/enzyme": "^3.1.13",
        "@types/jest": "^23.3.1",
        "@types/lodash": "^4.14.116",
        "@types/node": "^10.7.0",
        "@types/react": "^16.4.10",
        "@types/react-dom": "^16.0.7",
        "@types/react-loadable": "^5.4.1",
        "@types/react-router-dom": "^4.3.0",
        "@types/webpack-env": "^1.13.5",
        "autoprefixer": "^9.1.1",
        "awesome-typescript-loader": "^5.2.0",
        "babel-core": "^6.26.3",
        "babel-plugin-import": "^1.8.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-polyfill": "^6.26.0",
        "cache-loader": "^1.2.2",
        "classnames": "^2.2.6",
        "cross-env": "^5.1.4",
        "css-loader": "^1.0.0",
        "customaddcomponents": "^1.0.35",
        "enzyme": "^3.5.0",
        "enzyme-adapter-react-16": "^1.3.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.0.6",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^23.5.0",
        "less": "^3.8.0",
        "less-loader": "^4.1.0",
        "mini-css-extract-plugin": "^0.4.1",
        "moment-locales-webpack-plugin": "^1.0.7",
        "node-sass": "^4.9.2",
        "optimize-css-assets-webpack-plugin": "^5.0.0",
        "postcss-loader": "^3.0.0",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "stylelint": "^9.1.1",
        "stylelint-config-css-modules": "^1.2.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-config-ydj": "^0.1.1",
        "stylelint-order": "^0.8.1",
        "ts-import-plugin": "^1.5.4",
        "ts-jest": "^23.1.4",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslib": "^1.9.0",
        "tslint": "^5.11.0",
        "tslint-react": "^3.5.1",
        "typescript": "^3.0.1",
        "typings-for-css-modules-loader": "^1.7.0",
        "uglify-js": "^3.4.7",
        "url-loader": "^1.0.1",
        "webpack": "^4.16.5",
        "webpack-bundle-analyzer": "^2.11.1",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.5",
        "webpack-hot-middleware": "^2.22.3"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 11"
    ],
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy",
            "^@components(.*)$": "<rootDir>/src/components$1",
            "^@constants(.*)$": "<rootDir>/src/constants$1",
            "^@services(.*)$": "<rootDir>/src/services$1",
            "^@utils(.*)$": "<rootDir>/src/utils$1",
            "^@assets(.*)$": "<rootDir>/src/assets$1",
            "^@views(.*)$": "<rootDir>/src/container/views$1",
            "^@shared(.*)$": "<rootDir>/src/container/shared$1"
        },
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testMatch": [
            "<rootDir>/__test__/**/?(*.)(spec|test).ts?(x)"
        ]
    }
}
